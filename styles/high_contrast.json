{
  "name": "High Contrast",
  "description": "WCAG AAA compliant high-contrast theme for maximum accessibility.",
  "tokens": {
    "background": "#FFFFFF",
    "surface": "#F9FAFB",
    "foreground": "#000000",
    "foregroundSecondary": "#1F2937",
    "border": "#000000",
    "primary": "#0000FF",
    "primaryForeground": "#FFFFFF",
    "secondary": "#008000",
    "accent": "#FF6600",
    "error": "#CC0000",
    "warning": "#CC6600",
    "info": "#0066CC",
    "success": "#006600"
  },
  "typography": {
    "fontFamily": "Arial, sans-serif",
    "fontSizeBase": 18,
    "headingScale": [2.5, 2.0, 1.75, 1.5, 1.25, 1.0],
    "lineHeight": 1.75
  },
  "elevation": {
    "level0": "none",
    "level1": "0px 0px 0px 2px #000000",
    "level2": "0px 0px 0px 3px #000000",
    "level3": "0px 0px 0px 4px #000000"
  },
  "components": {
    "button": {
      "padding": "0.75rem 1.5rem",
      "radius": "0.25rem",
      "fontWeight": 700
    },
    "card": {
      "radius": "0.25rem",
      "padding": "2rem",
      "border": "3px solid #000000"
    },
    "input": {
      "padding": "0.875rem 1.25rem",
      "radius": "0.25rem",
      "border": "2px solid #000000"
    }
  }
}

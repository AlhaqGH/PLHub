{
  "platform": "android",
  "template_version": "1.0.0",
  "min_sdk": 24,
  "target_sdk": 34,
  "structure": {
    "app": {
      "src": {
        "main": {
          "poh": ["MainActivity.poh", "App.poh"],
          "res": {
            "layout": ["activity_main.xml"],
            "values": ["strings.xml", "colors.xml", "themes.xml"],
            "drawable": [],
            "mipmap-hdpi": [],
            "mipmap-mdpi": [],
            "mipmap-xhdpi": [],
            "mipmap-xxhdpi": [],
            "mipmap-xxxhdpi": []
          },
          "AndroidManifest.xml": true
        },
        "test": {
          "poh": ["ExampleUnitTest.poh"]
        },
        "androidTest": {
          "poh": ["ExampleInstrumentedTest.poh"]
        }
      },
      "build.gradle": true,
      "proguard-rules.pro": true
    },
    "gradle": {
      "wrapper": {
        "gradle-wrapper.jar": true,
        "gradle-wrapper.properties": true
      }
    },
    "build.gradle": true,
    "settings.gradle": true,
    "gradle.properties": true,
    "gradlew": true,
    "gradlew.bat": true,
    "local.properties": true
  },
  "dependencies": [
    "androidx.appcompat:appcompat:1.6.1",
    "androidx.constraintlayout:constraintlayout:2.1.4",
    "com.google.android.material:material:1.11.0",
    "androidx.lifecycle:lifecycle-runtime-ktx:2.7.0"
  ]
}
